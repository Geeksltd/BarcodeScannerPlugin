<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Zebble.BarcodeScanner</id>
    <version>1.2.109</version>
    <title>Barcode Scanner for Zebble</title>
    <projectUrl>https://github.com/Geeksltd/Zebble.BarcodeScanner</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/Geeksltd/Zebble.BarcodeScanner/master/Shared/NuGet/Icon.png</iconUrl>
    <copyright>Copyright © 2019 Geeks Ltd - All rights reserved.</copyright>
    <description>A plugin to allow scanning barcodes for Zebble for Xamarin projects.</description>
    <authors>Geeks Ltd</authors>
    <owners>Geeks Ltd</owners>
    <dependencies>
      <group targetFramework="MonoAndroid10">
        <dependency id="ZXing.Net.Mobile" version="2.3.2" />
        <dependency id="FastAndroidCamera" version="2.0.0" />
        <dependency id="Xamarin.Android.Support.v4" version="26.1.0.1" />
      </group>
      <group targetFramework="Xamarin.iOS10">
        <dependency id="ZXing.Net.Mobile" version="2.3.2" />
      </group>
      <group targetFramework="UAP10">
        <dependency id="ZXing.Net.Mobile" version="2.3.2" />
        <dependency id="ZXing.Net.Mobile.Forms" version="2.3.2" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\..\lib\Zebble.BarcodeScanner.iOS.*" target="lib\Xamarin.iOS10\" />
    <file src="..\..\lib\Zebble.BarcodeScanner.Android.*" target="lib\MonoAndroid10\" />
    <file src="..\..\lib\bin\ARM\Release\Zebble.BarcodeScanner.UWP.*" target="runtimes\win10-arm\lib\uap10.0" />
    <file src="..\..\lib\bin\x64\Release\Zebble.BarcodeScanner.UWP.*" target="runtimes\win10-x64\lib\uap10.0" />
    <file src="..\..\lib\bin\x86\Release\Zebble.BarcodeScanner.UWP.*" target="runtimes\win10-x86\lib\uap10.0" />
    <file src="..\..\lib\bin\Release\Zebble.BarcodeScanner.UWP.*" target="ref\uap10.0" />
  </files>
</package>