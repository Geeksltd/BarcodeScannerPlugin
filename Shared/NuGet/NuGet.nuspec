<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Zebble.BarcodeScanner</id>
    <version>1.0.1</version>
    <title>Barcode Scanner for Zebble</title>
    <description>A plugin to allow scanning barcodes for Zebble for Xamarin projects.</description>
    <authors>Mobile App Technologies Ltd</authors>
    <owners>Mobile App Technologies Ltd</owners>
    <projectUrl>https://github.com/Geeksltd/Zebble.BarcodeScanner</projectUrl>
	<iconUrl>https://raw.githubusercontent.com/Geeksltd/Zebble.BarcodeScanner/master/Shared/NuGet/Icon.png</iconUrl>
    <copyright>Copyright Â©2017 Mobile App Technologies Ltd</copyright>
    <dependencies>
      <group targetFramework="MonoAndroid10">
        <dependency id="ZXing.Net.Mobile"  version="2.2.9" />
        <dependency id="FastAndroidCamera"  version="2.0.0" />
        <dependency id="Xamarin.Android.Support.v4"  version="25.3.1" />
      </group>
     
    </dependencies>
  </metadata>
  
  <files>
    <file src="lib\BarcodeScanner.iOS.*" target="lib\Xamarin.iOS10\" />
    <file src="lib\BarcodeScanner.Android.*" target="lib\MonoAndroid10\" />
    <file src="lib\BarcodeScanner.UWP.*" target="lib\UAP10\" />
    
    <!-- UWP reference assemblies -->
    <file src="lib\BarcodeScanner.UWP\zxing.dll" target="lib\UAP10\zxing.dll" />  
  </files>
</package>